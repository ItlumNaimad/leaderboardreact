{
  "name": "leaderboardreact",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:up": "docker-compose up -d",
    "db:down": "docker-compose down",
    "db:restart": "docker-compose restart",
    "db:logs": "docker-compose logs -f db",
    "db:psql": "docker-compose exec db psql -U admin -d leaderboard_db",
    "db:reset": "docker-compose down -v && docker-compose up -d"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "next": "15.3.5"
  },
  "devDependencies": {
    "typescript": "^5",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@tailwindcss/postcss": "^4",
    "tailwindcss": "^4",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "@eslint/eslintrc": "^3"
  }
}
